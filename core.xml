   <project name="eii-c-utils" path="IEdgeInsights/common/util/c" />
   <project name="eii-messagebus" path="IEdgeInsights/common/libs/EIIMessageBus" />

   <!-- EII Sample Apps -->
   <project name="eii-samples" path="IEdgeInsights/Samples" />
</manifest>
